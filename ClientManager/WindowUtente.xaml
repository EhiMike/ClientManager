        <ComboBox x:Name="comboBoxProvincia" Grid.Column="5" HorizontalAlignment="Left" Height="30" Margin="1,50,0,0" Grid.Row="4" VerticalAlignment="Top" Width="118"/>
        <ComboBox x:Name="comboBoxStato" Grid.Column="5" HorizontalAlignment="Left" Height="30" Margin="176,49,0,0" Grid.Row="4" VerticalAlignment="Top" Width="118"/>
        <TextBox x:Name="textBoxMail" CharacterCasing="Lower" HorizontalAlignment="Left" Height="30" Margin="155,84,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="653" Grid.ColumnSpan="6" Grid.Row="4"  VerticalContentAlignment="Center"/>
        <TextBox x:Name="textBoxTelefono" CharacterCasing="Upper" HorizontalAlignment="Left" Height="30" Margin="155,119,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="654" Grid.ColumnSpan="6" Grid.Row="4"  VerticalContentAlignment="Center"/>
        <DatePicker x:Name="datePickerAbb" Grid.ColumnSpan="4" HorizontalAlignment="Left" Height="30" Margin="155,154,0,0" VerticalAlignment="Top" Width="231" Grid.Row="4"/>
        <DatePicker x:Name="datePickerVM" HorizontalAlignment="Left" Height="30" Margin="48,154,0,0" VerticalAlignment="Top" Width="245" Grid.Row="4" Grid.Column="5"/>
        <Button x:Name="btnConferma" Grid.ColumnSpan="3" Content="Conferma" Grid.Column="1" HorizontalAlignment="Left" Height="29" Margin="150,253,0,0" Grid.Row="4" VerticalAlignment="Top" Width="80" Click="btnConferma_Click"/>
        <Button x:Name="btnAnnulla" Content="Annulla" Grid.Column="4" HorizontalAlignment="Left" Height="29" Margin="10,253,0,0" Grid.Row="4" VerticalAlignment="Top" Width="80" Click="btnAnnulla_Click"/>
        <Label x:Name="lblCartaIdentita" Content="Carta Identità" HorizontalAlignment="Left" Height="30" Margin="5,189,0,0" VerticalAlignment="Top" Width="145" Grid.Row="4"/>
        <Label x:Name="lblVisita_Copy" Content="Visita Medica" HorizontalAlignment="Left" Height="30" Margin="17,189,0,0" VerticalAlignment="Top" Width="145" Grid.Row="4" Grid.ColumnSpan="2" Grid.Column="4"/>
        <Button x:Name="btnCaricaCI" Grid.ColumnSpan="3" Content="Carica " HorizontalAlignment="Left" Height="30" Margin="112,189,0,0" Grid.Row="4" VerticalAlignment="Top" Width="109" Grid.Column="1" Click="btnCaricaCI_Click"/>
        <Button x:Name="btnCaricaVM" Content="Carica" HorizontalAlignment="Left" Height="30" Margin="185,189,0,0" Grid.Row="4" VerticalAlignment="Top" Width="109" Grid.Column="5" Click="btnCaricaVM_Click"/>
        <Label x:Name="lblCIPresente" Content="No" HorizontalAlignment="Left" Height="30" Margin="155,189,0,0" VerticalAlignment="Top" Width="117" Grid.Row="4" Grid.ColumnSpan="2"/>
        <Label x:Name="lblPresenteVM" Content="No" HorizontalAlignment="Left" Height="30" Margin="48,189,0,0" VerticalAlignment="Top" Width="132" Grid.Row="4" Grid.Column="5"/>

    </Grid>
</Window>
