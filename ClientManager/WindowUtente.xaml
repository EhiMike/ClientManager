        <Label x:Name="lblNome" Content="Nome" HorizontalAlignment="Left" Margin="5,7,0,249" Width="145" Grid.Row="1" Grid.RowSpan="4"/>
        <Label x:Name="lblCognome" Content="Cognome" HorizontalAlignment="Left" Height="30" Margin="5,32,0,0" VerticalAlignment="Top" Width="145" Grid.Row="2" Grid.RowSpan="3"/>
        <Label x:Name="lblSesso" Content="Sesso" HorizontalAlignment="Left" Margin="15,7,0,6" Width="105" Grid.Column="4" Grid.Row="1" Grid.RowSpan="2" Grid.ColumnSpan="2"/>
        <Label x:Name="lblCF" Content="Codice Fiscale" Height="30" Margin="5,14,156,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Grid.Row="4"/>
        <Label x:Name="lblDataNascita" Content="Data di Nascita" HorizontalAlignment="Left" Height="30" Margin="15,32,0,0" VerticalAlignment="Top" Width="105" Grid.Column="4" Grid.Row="2" Grid.RowSpan="3"/>
        <Label x:Name="lblLuogoNascita" Content="Luogo di nascita" HorizontalAlignment="Right" Height="30" Margin="0,14,302,0" VerticalAlignment="Top" Width="105" Grid.Column="4" Grid.Row="4" Grid.ColumnSpan="2"/>
        <Label x:Name="lblIndirizzo" Content="Indirizzo" HorizontalAlignment="Left" Height="30" Margin="5,49,0,0" VerticalAlignment="Top" Width="145" Grid.Row="4"/>
        <Label x:Name="lblStato" Content="Stato" HorizontalAlignment="Left" Height="30" Margin="124,49,0,0" VerticalAlignment="Top" Width="47" Grid.Column="5" Grid.Row="4"/>
        <Label x:Name="lblProvincia" Content="Provincia" HorizontalAlignment="Left" Height="30" Margin="15,49,0,0" VerticalAlignment="Top" Width="105" Grid.Column="4" Grid.Row="4"/>
        <Label x:Name="lblEmail" Content="Email" HorizontalAlignment="Left" Height="30" Margin="5,84,0,0" VerticalAlignment="Top" Width="145" Grid.Row="4"/>
        <Label x:Name="lblTelefono" Content="Telefono" HorizontalAlignment="Left" Height="30" Margin="5,119,0,0" VerticalAlignment="Top" Width="145" Grid.Row="4"/>
        <Label x:Name="lblScadAbb" Content="Scadenza Abbonamento" HorizontalAlignment="Left" Height="30" Margin="5,154,0,0" VerticalAlignment="Top" Width="145" Grid.Row="4"/>
        <Label x:Name="lblVisita" Content="Scadenza Visita Medica" HorizontalAlignment="Left" Height="30" Margin="17,154,0,0" VerticalAlignment="Top" Width="145" Grid.Row="4" Grid.ColumnSpan="2" Grid.Column="4"/>
        <TextBox x:Name="textBoxNome" CharacterCasing="Upper" HorizontalAlignment="Left" Height="30" Margin="155,7,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="231" Grid.ColumnSpan="4" Grid.Row="1" Grid.RowSpan="2" VerticalContentAlignment="Center"/>
        <TextBox x:Name="textBoxCognome" CharacterCasing="Upper" HorizontalAlignment="Left" Height="30" Margin="155,32,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="231" Grid.ColumnSpan="4" Grid.Row="2" Grid.RowSpan="3" VerticalContentAlignment="Center"/>
        <TextBox x:Name="textBoxCF" CharacterCasing="Upper" HorizontalAlignment="Left" Height="30" Margin="155,14,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="231" Grid.ColumnSpan="4" Grid.Row="4" MaxLength="16" VerticalContentAlignment="Center"/>
        <RadioButton x:Name="rbSessoM" Content="M" Grid.Column="5" HorizontalAlignment="Left" Height="21" Margin="5,6,0,0" VerticalAlignment="Top" Width="65" Grid.Row="2" GroupName="radioGroupSesso"/>
        <RadioButton x:Name="rbSessoF" Content="F" Grid.Column="5" HorizontalAlignment="Left" Height="21" Margin="90,6,0,0" VerticalAlignment="Top" Width="65" Grid.Row="2" GroupName="radioGroupSesso"/>
